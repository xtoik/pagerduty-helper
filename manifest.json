{
    "manifest_version": 3,
    "name": "PagerDuty Helpers",
    "description": "Extension that extends the funtionalities for PagerDuty",
    "version": "1.0",
    "permissions": ["scripting", "contextMenus"],
    "host_permissions": ["https://*.pagerduty.com/*"],
    "action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
  }